{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npm run build:web",
    "serve": "ng serve",
    "start": "node server.js",
    "bin:electron:linux": "rm -rf ./electron && npx cap add electron && cap copy electron && electron-packager ./electron --platform linux --arch x64 --out dist/",
    "deb64:electron:linux": "electron-installer-debian --src dist/frontend-linux-x64 --dest dist/installers/ --arch amd64 ",
    "build:web": "ng build",
    "build:android": "npx cap add android && npx cap copy android",
    "build:electron:linux": "npm run bin:electron:linux && npm run deb64:electron:linux"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.6",
    "@angular/cdk": "^9.2.3",
    "@angular/cli": "~9.0.6",
    "@angular/common": "~9.0.6",
    "@angular/compiler": "~9.0.6",
    "@angular/compiler-cli": "~9.0.6",
    "@angular/core": "~9.0.6",
    "@angular/forms": "~9.0.6",
    "@angular/material": "^9.2.2",
    "@angular/platform-browser": "~9.0.6",
    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/router": "~9.0.6",
    "@capacitor/android": "^2.2.0",
    "@capacitor/cli": "^2.2.0",
    "@capacitor/core": "^2.2.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "jwt-decode": "^2.2.0",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.10.0",
    "typescript": "~3.7.5",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular/language-service": "~9.0.6",
    "@types/crypto-js": "^3.1.47",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^1.4.33",
    "codelyzer": "^5.1.2",
    "electron": "^9.2.0",
    "electron-installer-debian": "^3.1.0",
    "electron-packager": "^15.0.0",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0"
  },
  "engines": {
    "node": "13.5.0",
    "npm": "6.13.4"
  }
}
